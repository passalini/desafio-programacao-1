<tr id="report-<%= report.id %>" class="report">
  <td><%= link_to report.name, report_path(report) %></td>
  <td><%= report.aasm_state %></td>
  <td class="income"><%= number_to_currency(report.income) %></td>
  <td>
    <%= link_to 'Download', url_for(report.file) %>
    <%= link_to 'Edit', edit_report_path(report) %>
    <%= link_to 'Destroy', report, method: :delete, data: { confirm: 'Are you sure?' } %>
  </td>
</tr>
